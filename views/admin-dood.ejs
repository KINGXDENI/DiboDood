    <%- include('template/footer.ejs') %>
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            // Function to close the URL modal
            function closeURLModal() {
                document.getElementById('showModalURL').classList.add('hidden');
            }

            // Event listener to close the URL modal when the close button is clicked
            document.querySelector('[data-modal-close="showModalURL"]').addEventListener('click',
                closeURLModal);

            // Event listener for URL form submission
            document.getElementById('url-form').addEventListener('submit', function (event) {
                event.preventDefault();
                const url = document.getElementById('url-field').value;

                fetch('/addvideo', {
                        method: 'POST',
                        headers: {
                            'Content-Type': 'application/json'
                        },
                        body: JSON.stringify({
                            url
                        })
                    })
                    .then(response => {
                        if (response.ok) {
                            // Show success alert
                            Swal.fire({
                                title: 'Success!',
                                text: 'Video has been added successfully.',
                                icon: 'success',
                                confirmButtonText: 'OK'
                            }).then(() => {
                                closeURLModal();
                                window.location
                            .reload(); // Reload the page or update UI accordingly
                            });
                        } else {
                            // Show error alert
                            Swal.fire({
                                title: 'Error!',
                                text: 'Failed to add video. Please try again.',
                                icon: 'error',
                                confirmButtonText: 'OK'
                            });
                        }
                    })
                    .catch(error => {
                        // Show error alert
                        Swal.fire({
                            title: 'Error!',
                            text: 'Failed to add video. Please try again.',
                            icon: 'error',
                            confirmButtonText: 'OK'
                        });
                        console.error('Error adding video:', error);
                    });
            });
            window.handleEdit = function (id) {

                // Fetch video details from server
                fetch(`/videos/${id}`)
                    .then(response => {
                        if (!response.ok) throw new Error('Failed to fetch video details');
                        return response.json();
                    })
                    .then(video => {
                        if (!video) {
                            console.error('Video not found');
                            return;
                        }

                        // Show SweetAlert2 prompt for editing
                        Swal.fire({
                            title: 'Edit Video',
                            html: `
            <input id="swal-title" class="swal2-input" placeholder="Title" value="${video.title}">
            <input id="swal-thumbnail" class="swal2-input" placeholder="Thumbnail URL" value="${video.thumbnail}">
            <input id="swal-duration" class="swal2-input" placeholder="Duration" value="${video.duration}">
            <input id="swal-views" class="swal2-input" type="number" placeholder="Views" value="${video.views}">
            <input id="swal-postedAt" class="swal2-input" type="date" placeholder="Posted At"
                value="${new Date(video.postedAt).toISOString().split('T')[0]}">
            <input id="swal-channel" class="swal2-input" placeholder="Channel" value="${video.channel.name}">
            `,
                            focusConfirm: false,
                            preConfirm: () => {
                                return {
                                    title: document.getElementById('swal-title').value,
                                    thumbnail: document.getElementById('swal-thumbnail')
                                        .value,
                                    duration: document.getElementById('swal-duration')
                                        .value,
                                    views: document.getElementById('swal-views').value,
                                    postedAt: document.getElementById('swal-postedAt')
                                        .value,
                                    channel: document.getElementById('swal-channel').value
                                };
                            }
                        }).then(result => {
                            if (result.isConfirmed) {
                                const updatedVideo = result.value;

                                // Send updated video data to server
                                fetch(`/videos/${id}`, {
                                        method: 'PUT',
                                        headers: {
                                            'Content-Type': 'application/json'
                                        },
                                        body: JSON.stringify(updatedVideo)
                                    })
                                    .then(response => {
                                        if (!response.ok) throw new Error(
                                            'Failed to update video');
                                        Swal.fire('Updated!', 'Video has been updated.',
                                            'success').then(() => {
                                            window.location
                                                .reload(); // Reload the page or update UI
                                        });
                                    })
                                    .catch(error => {
                                        Swal.fire('Error!', 'Failed to update video.',
                                            'error');
                                        console.error('Error updating video:', error);
                                    });
                            }
                        });
                    })
                    .catch(error => {
                        Swal.fire('Error!', 'Failed to fetch video details.', 'error');
                        console.error('Error fetching video details:', error);
                    });
            };
            // Function to handle delete
            window.handleDelete = function (url) { // Ensure function is attached to window object
                Swal.fire({
                    title: 'Are you sure?',
                    text: "This action cannot be undone.",
                    icon: 'warning',
                    showCancelButton: true,
                    confirmButtonColor: '#3085d6',
                    cancelButtonColor: '#d33',
                    confirmButtonText: 'Yes, delete it!'
                }).then((result) => {
                    if (result.isConfirmed) {
                        fetch(url, {
                            method: 'DELETE'
                        }).then(response => {
                            if (response.ok) {
                                Swal.fire(
                                    'Deleted!',
                                    'Your file has been deleted.',
                                    'success'
                                ).then(() => {
                                    window.location
                                        .reload(); // Reload the page or update UI accordingly
                                });
                            } else {
                                console.error('Error deleting video');
                                Swal.fire(
                                    'Error!',
                                    'There was an error deleting the video.',
                                    'error'
                                );
                            }
                        }).catch(error => {
                            console.error('Error deleting video', error);
                            Swal.fire(
                                'Error!',
                                'There was an error deleting the video.',
                                'error'
                            );
                        });
                    }
                });
            }
        });
    </script>